<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddProductController">
   <children>
      <Label layoutX="152.0" layoutY="31.0" prefHeight="56.0" prefWidth="256.0" text="%addProductWindow.label.title">
         <font>
            <Font name="Dubai Regular" size="24.0" />
         </font>
      </Label>
      <ComboBox fx:id="categoryComboBox" layoutX="52.0" layoutY="121.0" prefWidth="150.0" promptText="%addProductWindow.comboBox.category" />
      <TextArea fx:id="notesTextArea" layoutX="356.0" layoutY="134.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="365.0" layoutY="104.0" text="%addProductWindow.label.notes">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="addProductButton" layoutX="479.0" layoutY="354.0" mnemonicParsing="false" onAction="#addThisProduct" text="%addProductWindow.button" />
      <CheckBox fx:id="leaveOnCheckBox" layoutX="177.0" layoutY="299.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="120.0" text="%addProductWindow.checkBox.leaveOn" />
      <CheckBox fx:id="coWashCheckBox" layoutX="52.0" layoutY="299.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="120.0" text="%addProductWindow.checkBox.coWash" />
      <TextField fx:id="nameTextField" layoutX="52.0" layoutY="253.0" prefHeight="25.0" prefWidth="250.0" />
      <ComboBox fx:id="consistencyComboBox" layoutX="50.0" layoutY="175.0" prefWidth="150.0" promptText="%addProductWindow.comboBox.consistency" />
      <ComboBox fx:id="strongLevelComboBox" layoutX="50.0" layoutY="175.0" prefWidth="150.0" promptText="%addProductWindow.comboBox.strongLevel" />
      <ComboBox fx:id="typeComboBox" layoutX="50.0" layoutY="175.0" prefWidth="150.0" promptText="%addProductWindow.comboBox.type" />
      <Label layoutX="52.0" layoutY="226.0" text="%addProductWindow.label.name">
         <font>
            <Font name="Dubai Regular" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
